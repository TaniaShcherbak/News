{"ast":null,"code":"import newsPost from './News_Post.vue';\nexport default {\n  name: 'postPanel',\n  data() {\n    return {\n      title: [\"1\", \"2\", \"3\", \"4\", \"5\", \"6\", \"7\", \"4\", \"8\", \"9\", \"10\"],\n      desc: ['some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc'],\n      posts: null\n    };\n  },\n  components: {\n    newsPost\n  },\n  mounted() {\n    var url = 'https://newsapi.org/v2/everything?' + 'q=Apple&' + 'from=2023-08-18&' + 'sortBy=popularity&' + 'apiKey=b31fcdc8b21948dab1c86180ea11f5fb';\n    var req = new Request(url);\n    fetch(req).then(response => {\n      return response.json(); // Розпарсувати відповідь в форматі JSON posts.articles[i].title\n    }).then(data => {\n      this.posts = data; // Зберегти дані в об'єкті даних компонента\n      console.log(this.posts); // Дані JSON доступні тут\n      JSON.stringify(this.posts);\n      console.log(this.posts.articles[0].title);\n    }).catch(error => {\n      console.log('Помилка: ' + error);\n    });\n  }\n};","map":{"version":3,"names":["newsPost","name","data","title","desc","posts","components","mounted","url","req","Request","fetch","then","response","json","console","log","JSON","stringify","articles","catch","error"],"sources":["D:\\learn_with_panchenko\\src\\components\\PostBoard.vue"],"sourcesContent":["<template>\r\n    <div class=\"post-board\">\r\n        <div v-if=\"\">\r\n            <newsPost v-for=\"(index, i) in 10\" :key=\"i\"\r\n            :postinfo=\"{ title: posts.articles[i].title, content: desc[i], image: require('@/assets/204.jpg') }\" />\r\n        </div>\r\n        \r\n        <button @click=\"getPosts\">Get Posts</button>\r\n        <!-- <newsPost :postinfo=\"{ title: 'firstPost', description: 'some desc', image: require('@/assets/204.jpg') }\" /> `@/assets/${i+1}.jpg` -->\r\n    </div>\r\n</template>\r\n<script>\r\nimport newsPost from './News_Post.vue';\r\nexport default {\r\n    name: 'postPanel',\r\n    data() {\r\n        return {\r\n            title: [\"1\", \"2\", \"3\", \"4\", \"5\", \"6\", \"7\", \"4\", \"8\", \"9\", \"10\"],\r\n            desc: ['some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc', 'some desc'],\r\n            posts: null,\r\n        }\r\n    },\r\n    components: {\r\n        newsPost,\r\n    },\r\n    mounted() {\r\n        var url = 'https://newsapi.org/v2/everything?' +\r\n            'q=Apple&' +\r\n            'from=2023-08-18&' +\r\n            'sortBy=popularity&' +\r\n            'apiKey=b31fcdc8b21948dab1c86180ea11f5fb';\r\n\r\n        var req = new Request(url);\r\n\r\n        fetch(req)\r\n            .then((response) => {\r\n                return response.json(); // Розпарсувати відповідь в форматі JSON posts.articles[i].title\r\n            })\r\n            .then((data) => {\r\n                this.posts = data; // Зберегти дані в об'єкті даних компонента\r\n                console.log(this.posts); // Дані JSON доступні тут\r\n                JSON.stringify(this.posts);\r\n                console.log(this.posts.articles[0].title); \r\n            })\r\n            .catch((error) => {\r\n                console.log('Помилка: ' + error);\r\n            });\r\n    }\r\n}\r\n</script>\r\n<style>\r\n.post-board {\r\n    display: flex;\r\n    flex-direction: row;\r\n    flex-wrap: wrap;\r\n}\r\n</style>"],"mappings":"AAYA,OAAOA,QAAO,MAAO,iBAAiB;AACtC,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,CAAC,GAAG,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG,EAAE,IAAI,CAAC;MAC/DC,IAAI,EAAE,CAAC,WAAW,EAAE,WAAW,EAAE,WAAW,EAAE,WAAW,EAAE,WAAW,EAAE,WAAW,EAAE,WAAW,EAAE,WAAW,EAAE,WAAW,EAAE,WAAW,CAAC;MACxIC,KAAK,EAAE;IACX;EACJ,CAAC;EACDC,UAAU,EAAE;IACRN;EACJ,CAAC;EACDO,OAAOA,CAAA,EAAG;IACN,IAAIC,GAAE,GAAI,oCAAmC,GACzC,UAAS,GACT,kBAAiB,GACjB,oBAAmB,GACnB,yCAAyC;IAE7C,IAAIC,GAAE,GAAI,
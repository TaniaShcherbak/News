rue,\r\n      default: () => {\r\n        return { name: \"without number\", date: \"today\" };\r\n      },\r\n    },\r\n    test: {\r\n      type: Number,\r\n    },\r\n  },\r\n  mounted() {\r\n    (this.testCopy = this.test), (this.testCopy = 44);\r\n  },\r\n  data() {\r\n    return {\r\n      testCopy: \"\",\r\n      ClassName: \"my-first-component\",\r\n      counter: 0,\r\n      hoverCounter: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    onClickCounter() {\r\n      this.consolog(this.counter);\r\n      this.counter++;\r\n      this.$emit(\"clickCounter\", this.onClickCounter)\r\n    },\r\n    consolog(number) {\r\n      console.log(number);\r\n    },\r\n    onMouseOver(event) {\r\n      this.hoverCounter++;\r\n      console.log(event);\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.pointer {\r\n  cursor: pointer;\r\n}\r\n\r\n.border {\r\n  border: 2px solid black;\r\n  width: 50px;\r\n  margin: 0 auto;\r\n}\r\n</style>\r\n"],"mappings":"AAuBA,eAAe;EACbA,IAAI,EAAE,eAAe;EACrBC,KAAK,EAAE;IACLC,IAAI,EAAE;MACJC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAEA,CAAA,KAAM;QACb,OAAO;UAAEN,IAAI,EAAE,gBAAgB;UAAEO,IAAI,EAAE;QAAQ,CAAC;MAClD;IACF,CAAC;IACDC,IAAI,EAAE;MACJL,IAAI,EAAEM;IACR;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACP,IAAI,CAACC,QAAO,GAAI,IAAI,CAACH,IAAI,EAAI,IAAI,CAACG,QAAO,GAAI,EAAG;EACnD,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLD,QAAQ,EAAE,EAAE;MACZE,SAAS,EAAE,oBAAoB;MAC/BC,OAAO,EAAE,CAAC;MACVC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,IAAI,CAACC,QAAQ,CAAC,IAAI,CAACJ,OAAO,CAAC;MAC3B,IAAI,CAACA,OAAO,EAAE;MACd,IAAI,CAACK,KAAK,CAAC,cAAc,EAAE,IAAI,CAACF,cAAc;IAChD,CAAC;IACDC,QAAQA,CAACE,MAAM,EAAE;MACfC,OAAO,CAACC,GAAG,CAACF,MAAM,CAAC;IACrB,CAAC;IACDG,WAAWA,CAACC,KAAK,EAAE;MACjB,IAAI,CAACT,YAAY,EAAE;MACnBM,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;IACpB;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}
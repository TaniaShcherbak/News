erry=newQ :amount=amountPost />\n    </div>\n</template>\n\n<script>\nimport postPanel from \"./components/PostBoard.vue\"\nimport searchBar from \"./components/search.vue\"\nimport navPanel from \"./components/NavPanel.vue\"\nexport default {\n    name: \"App\",\n    data() {\n        return {\n            newQ: \"Apple\",\n            amountPost:8\n        }\n    },\n    components: {\n        postPanel,\n        searchBar,\n        navPanel\n    },\n    watch: {\n        amountItemsOnPage: 'changeAmount',\n    },\n    methods: {\n        changeQuerry(data) {\n            this.newQ=data;\n        },\n        changeAmount(data){\n            this.amountPost=data;\n            console.log(this.amountPost);\n        }\n    },\n    computed: {\n        computedValue() {\n            return this.newQuerry;\n        }\n    },\n}\n// import HelloWorld from \"./components/HelloWorld.vue\";\n// import PostComponent from \"./components/Post.vue\";\n// import Counter from \"./components/Counter.vue\"\n// export default {\n//   name: \"App\",\n//   components: {\n//     HelloWorld,\n//     PostComponent,\n//     Counter\n//   },\n//   data() {\n//     return {\n//       globalCounter: 0,\n//     }\n//   },\n//   methods: {\n//     onPostClickCounter(counter, id) {\n//       this.globalCounter=counter;\n//     }\n//   }\n// };\n</script>\n\n"],"mappings":"AAeA,OAAOA,SAAQ,MAAO,4BAA2B;AACjD,OAAOC,SAAQ,MAAO,yBAAwB;AAC9C,OAAOC,QAAO,MAAO,2BAA0B;AAC/C,eAAe;EACXC,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,OAAO;MACbC,UAAU,EAAC;IACf;EACJ,CAAC;EACDC,UAAU,EAAE;IACRP,SAAS;IACTC,SAAS;IACTC;EACJ,CAAC;EACDM,KAAK,EAAE;IACHC,iBAAiB,EAAE;EACvB,CAAC;EACDC,OAAO,EAAE;IACLC,YAAYA,CAACP,IAAI,EAAE;MACf,IAAI,CAACC,IAAI,GAACD,IAAI;IAClB,CAAC;IACDQ,YAAYA,CAACR,IAAI,EAAC;MACd,IAAI,CAACE,UAAU,GAACF,IAAI;MACpBS,OAAO,CAACC,GAAG,CAAC,IAAI,CAACR,UAAU,CAAC;IAChC;EACJ,CAAC;EACDS,QAAQ,EAAE;IACNC,aAAaA,CAAA,EAAG;MACZ,OAAO,IAAI,CAACC,SAAS;IACzB;EACJ;AACJ;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;
{"ast":null,"code":"import newsPost from './News_Post.vue';\nexport default {\n  name: 'postPanel',\n  props: {\n    newQuerry: {\n      type: String,\n      required: true,\n      default: () => {\n        return \"Apple\";\n      }\n    }\n  },\n  data() {\n    return {\n      posts: null,\n      selectedPost: null,\n      page: 1,\n      total: 0\n    };\n  },\n  components: {\n    newsPost\n  },\n  watch: {\n    newQuerry: 'fetchData'\n  },\n  methods: {\n    fetchData() {\n      let querry = 'q=' + this.newQuerry + \"&\";\n      var url = 'https://newsapi.org/v2/everything?' + querry + 'from=2023-08-19&' + 'sortBy=popularity&' + 'apiKey=b31fcdc8b21948dab1c86180ea11f5fb';\n      var req = new Request(url);\n      fetch(req).then(response => {\n        return response.json();\n      }).then(data => {\n        this.posts = data;\n        JSON.stringify(this.posts);\n        this.total = this.posts.count;\n        console.log(this.posts.articles.length);\n      }).catch(error => {\n        console.log('Помилка: ' + error);\n      });\n    }\n  },\n  mounted() {\n    this.fetchData();\n  }\n};","map":{"version":3,"names":["newsPost","name","props","newQuerry","type","String","required","default","data","posts","selectedPost","page","total","components","watch","methods","fetchData","querry","url","req","Request","fetch","then","response","json","JSON","stringify","count","console","log","articles","length","catch","error","mounted"],"sources":["D:\\learn_with_panchenko\\src\\components\\PostBoard.vue"],"sourcesContent":["<template>\r\n    <div class=\"panel\">\r\n        <div v-if=\"posts\" class=\"post-board\">\r\n            <newsPost v-for=\"(index, i) in 10\" :key=\"i\" :postinfo=\"{\r\n                title: posts.articles[i].title, desc: posts.articles[i].description, content: posts.articles[i].content\r\n                , image: posts.articles[i].urlToImage\r\n            }\" />\r\n        </div>\r\n        <div v-else>\r\n            <p>Sorry, try another request</p>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\nimport newsPost from './News_Post.vue';\r\nexport default {\r\n    name: 'postPanel',\r\n    props: {\r\n        newQuerry: {\r\n            type: String,\r\n            required: true,\r\n            default: () => {\r\n                return \"Apple\"\r\n            }\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            posts: null,\r\n            selectedPost: null,\r\n            page: 1,\r\n            total: 0\r\n        }\r\n    },\r\n    components: {\r\n        newsPost,\r\n    },\r\n    watch: {\r\n        newQuerry: 'fetchData'\r\n    },\r\n    methods: {\r\n        fetchData() {\r\n            let querry = 'q=' + this.newQuerry + \"&\";\r\n            var url = 'https://newsapi.org/v2/everything?' +\r\n                querry +\r\n                'from=2023-08-19&' +\r\n                'sortBy=popularity&' +\r\n                'apiKey=b31fcdc8b21948dab1c86180ea11f5fb';\r\n\r\n            var req = new Request(url);\r\n\r\n            fetch(req)\r\n                .then((response) => {\r\n                    return response.json();\r\n                })\r\n                .then((data) => {\r\n                    this.posts = data;\r\n                    JSON.stringify(this.posts);\r\n                    this.total = this.posts.count;\r\n                    console.log(this.posts.articles.length);\r\n                })\r\n                .catch((error) => {\r\n                    console.log('Помилка: ' + error);\r\n                });\r\n        }\r\n    },\r\n    mounted() {\r\n        this.fetchData();\r\n    }\r\n}\r\n</script>\r\n<style>\r\n.post-board {\r\n    display: flex;\r\n    flex-direction: row;\r\n    flex-wrap: wrap;\r\n}\r\n</style>"],"mappings":"AAcA,OAAOA,QAAO,MAAO,iBAAiB;AACtC,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACHC,SAAS,EAAE;MACPC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAEA,CAAA,KAAM;QACX,OAAO,OAAM;MACjB;IACJ;EACJ,CAAC;EACDC,IA
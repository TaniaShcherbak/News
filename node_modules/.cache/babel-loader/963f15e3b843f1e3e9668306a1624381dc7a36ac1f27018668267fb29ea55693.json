{"ast":null,"code":"import newsPost from './News_Post.vue';\nimport detailPost from './PostDetaoled.vue';\nexport default {\n  name: 'postPanel',\n  props: {\n    newQuerry: {\n      type: String,\n      required: true,\n      default: () => {\n        return \"Apple&\";\n      }\n    }\n  },\n  data() {\n    return {\n      posts: null,\n      selectedPost: null\n    };\n  },\n  components: {\n    newsPost,\n    detailPost\n  },\n  methods: {\n    showDetail(post) {\n      this.selectedPost = post;\n    },\n    closeDetail() {\n      this.$emit(\"closeDetail\");\n    },\n    fetchData() {\n      let querry = 'q=' + this.newQuerry;\n      console.log(querry);\n      var url = 'https://newsapi.org/v2/everything?' + querry + 'from=2023-08-19&' + 'sortBy=popularity&' + 'apiKey=b31fcdc8b21948dab1c86180ea11f5fb';\n      var req = new Request(url);\n      fetch(req).then(response => {\n        return response.json();\n      }).then(data => {\n        this.posts = data;\n        JSON.stringify(this.posts);\n      }).catch(error => {\n        console.log('Помилка: ' + error);\n      });\n    }\n  },\n  mounted() {\n    this.fetchData();\n  }\n};","map":{"version":3,"names":["newsPost","detailPost","name","props","newQuerry","type","String","required","default","data","posts","selectedPost","components","methods","showDetail","post","closeDetail","$emit","fetchData","querry","console","log","url","req","Request","fetch","then","response","json","JSON","stringify","catch","error","mounted"],"sources":["D:\\learn_with_panchenko\\src\\components\\PostBoard.vue"],"sourcesContent":["<template>\r\n    <div class=\"panel\">\r\n        <div v-if=\"posts\" class=\"post-board\">\r\n            <newsPost v-for=\"(index, i) in 10\" :key=\"i\" @click=\"showDetail(i)\" :postinfo=\"{\r\n                title: posts.articles[i].title, desc: posts.articles[i].description, content: posts.articles[i].content\r\n                , image: posts.articles[i].urlToImage\r\n            }\" />\r\n        </div>\r\n        <div v-else>\r\n            <p>Sorry, try another request</p>\r\n        </div>\r\n        <div v-if=\"selectedPost\">\r\n            <detailPost :post=\"posts.articles[selectedPost]\" @closeDetail=\"selectedPost = null\" />\r\n        </div>\r\n        <!-- <newsPost :postinfo=\"{ title: 'firstPost', description: 'some desc', image: require('@/assets/204.jpg') }\" /> `@/assets/${i+1}.jpg` -->\r\n    </div>\r\n</template>\r\n<script>\r\nimport newsPost from './News_Post.vue';\r\nimport detailPost from './PostDetaoled.vue'\r\nexport default {\r\n    name: 'postPanel',\r\n    props: {\r\n        newQuerry: {\r\n            type: String,\r\n            required: true,\r\n            default: () => {\r\n                return \"Apple&\"\r\n            }\r\n        }\r\n    },\r\n    data() {\r\n        return {\r\n            posts: null,\r\n            selectedPost: null,\r\n        }\r\n    },\r\n    components: {\r\n        newsPost,\r\n        detailPost\r\n    },\r\n    methods: {\r\n        showDetail(post) {\r\n            this.selectedPost = post;\r\n        },\r\n        closeDetail() {\r\n            this.$emit(\"closeDetail\");\r\n        },\r\n        fetchData(){\r\n            let querry = 'q=' + this.newQuerry;\r\n        console.log(querry);\r\n        var url = 'https://newsapi.org/v2/everything?' +\r\n            querry +\r\n            'from=2023-08-19&' +\r\n            'sortBy=popularity&' +\r\n            'apiKey=b31fcdc8b21948dab1c86180ea11f5fb';\r\n\r\n        var req = new Request(url);\r\n\r\n        fetch(req)\r\n            .then((response) => {\r\n                return response.json();\r\n            })\r\n            .then((data) => {\r\n                this.posts = data;\r\n                JSON.stringify(this.posts);\r\n            })\r\n            .catch((error) => {\r\n                console.log('Помилка: ' + error);\r\n            });\r\n        }\r\n    },\r\n    mounted() {\r\n        this.fetchData();\r\n    }\r\n}\r\n</script>\r\n<style>\r\n.post-board {\r\n    display: flex;\r\n    flex-direction: row;\r\n    flex-wrap: wrap;\r\n}\r\n</style>"],"mappings":"AAkBA,OAAOA,QAAO,MAAO,iBAAiB;AACtC,OAAOC,UAAS,MAAO,oBAAmB;AAC1C,eAAe;EACXC,IAAI,EAAE,WAAW;EACjBC,KAAK,EAAE;IACHC,SAAS,EAAE;MACPC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAEA,CAAA,KAAM;QACX,OAAO,QAAO;MAClB;IACJ;EACJ,CAAC;EACDC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,IAAI;MACXC,YAAY,EAAE;IAClB;EACJ,CAAC;EACDC,UAAU,EAAE;IACRZ,QAAQ;IACRC;EACJ,CAAC;EACDY,OAAO,EAAE;IACLC,UAAUA,CAACC,IAAI,EAAE;MACb,IAAI,CAACJ,YAAW,GAAII,IAAI;IAC5B,CAAC;IACDC,WAAWA,CAAA,EAAG;MACV,IAAI,CAACC,KAAK,CAAC,aAAa,CAAC;IAC7B,CAAC;IACDC,SAASA,CAAA,EAAE;MACP,IAAIC,MAAK,GAAI,IAAG,GAAI,IAAI,CAACf,SAAS;MACtCgB,OAAO,CAACC,GAAG,CAACF,MAAM,CAAC;MACnB,IAAIG,GAAE,GAAI,oCAAmC,GACzCH,MAAK,GACL,kBAAiB,GACjB,oBAAmB,GACnB,yCAAyC;MAE7C,IAAII,GAAE,GAAI,IAAIC,OAAO,CAACF,GAAG,CAAC;MAE1BG,KAAK,CAACF,GAAG,EACJG,IAAI,CAAEC,QAAQ,IAAK;QAChB,OAAOA,QAAQ,CAACC,IAAI,CAAC,CAAC;MAC1B,CAAC,EACAF,IAAI,CAAEjB,IAAI,IAAK;QACZ,IAAI,CAACC,KAAI,GAAID,IAAI;QACjBoB,IAAI,CAACC,SAAS,CAAC,IAAI,CAACpB,KAAK,CAAC;MAC9B,CAAC,EACAqB,KAAK,CAAEC,KAAK,IAAK;QACdZ,OAAO,CAACC,GAAG,CAAC,WAAU,GAAIW,KAAK,CAAC;MACpC,CAAC,CAAC;IACN;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACf,SAAS,CAAC,CAAC;EACpB;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}